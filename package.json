{
  "name": "newsly",
  "version": "1.0.0",
  "description": "common news feed for trello",
  "scripts": {
    "start": "node build/main.js",
    "babelify": "babel server/ -d build/",
    "prebuild": "node_modules/.bin/rimraf ./dist && mkdir dist",
    "build": "npm run prebuild && babel-node build.js",
    "server:dev": "nodemon --watch server --exec babel-node -- server/main.dev.js",
    "server:prod": "npm run prebuild && npm run build && babel-node server/main.prod.js"
  },
  "author": "mcmxc",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.14.0",
    "babel-polyfill": "6.8.0",
    "bluebird": "^3.4.6",
    "cookie-session": "^2.0.0-alpha.1",
    "date-and-time": "^0.3.0",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jquery": "2.2.3",
    "lodash": "^4.16.4",
    "mongoose": "^4.6.1",
    "node-trello": "^1.1.2",
    "octonode": "^0.7.6",
    "passport": "^0.3.2",
    "passport-trello": "^0.1.5",
    "query-string-parser": "^0.1.4",
    "react": "^15.3.2",
    "react-addons-update": "^15.3.1",
    "react-dom": "^15.3.2",
    "react-loading": "0.0.9",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.3",
    "react-router-redux": "^5.0.0-alpha.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.8.2",
    "webpack-hot-middleware": "^2.12.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "eventsource-polyfill": "^0.9.6",
    "nodemon": "^1.10.2"
  }
}
